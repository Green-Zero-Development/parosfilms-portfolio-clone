{% set address = [] %}
{% set phone = [] %}
{% set email = [] %}
{% set logos = [] %}
{% set social = [] %}
{% for item in globalData %}
    {% if item.title == "Physical Addresses" %}
        {% set address = (address.push({id: item.id, title: item.title, acf: item.acf}), address) %}
    {% elseif item.title == "Logos" %}
        {% set logos = (logos.push({id: item.id, title: item.title, acf: item.acf}), logos) %}
    {% elseif item.title == "Contact Numbers" %}
        {% set phone = (phone.push({id: item.id, title: item.title, acf: item.acf}), phone) %}
    {% elseif item.title == "Emails" %}
        {% set email = (email.push({id: item.id, title: item.title, acf: item.acf}), email) %}
    {% elseif item.title == "Social Media Links" %}
        {% set social = (social.push({id: item.id, title: item.title, acf: item.acf}), social) %}
    {% endif %}
{% endfor %}

<style>
    .pre-footer-cta {
        font-family: 'Newsreader', serif;
        font-weight: 100;
        font-size: 70px;
        letter-spacing: -4px;
        line-height: 0.6;
    }
    .pre-footer-title-line {
        background-color: #942308;
        height: 2px;
    }
    .privacy-text {
        font-family: 'commuters-sans', sans-serif;
        font-size: 8px;
        text-transform: uppercase;
        letter-spacing: 1.1px;
        color: #942308;
    }
    .footer-address {
        font-family: 'commuters-sans', sans-serif;
        font-weight: 700;
        font-size: 12px;
        text-transform: uppercase;
        letter-spacing: 1.15px;
        line-height: 1;
        color: #942308;
        font-style: normal;
    }
    .footer-contact {
        font-family: 'commuters-sans', sans-serif;
        font-weight: 700;
        font-size: 12px;
        text-transform: uppercase;
        letter-spacing: 1.15px;
        line-height: 1;
        color: #942308;
        font-style: normal;
    }
    .built-by-text {
        font-family: 'commuters-sans', sans-serif;
        font-weight: 700;
        font-size: 8px;
        text-transform: uppercase;
        letter-spacing: 1.15px;
        text-decoration: underline;
        color: #942308;
    }
    @media (min-width: 576px) {
        .pre-footer-cta {
            font-size: 100px !important;
        }
    }
    @media (min-width: 768px) {
        .pre-footer-cta {
            font-size: 170px !important;
            letter-spacing: -14px !important;
            line-height: 0.6 !important;
        }
    }
    @media (min-width: 992px) {
        .pre-footer-cta {
            font-size: 200px !important;
        }
    }
    @media (min-width: 1200px) {
        .pre-footer-cta {
            font-size: 250px !important;
        }
    }
    @media (min-width: 1440px) {
        .pre-footer-cta {
            font-size: 308px !important;
        }
    }
</style>

<footer class="relative pt-80 pb-32 lg:pb-0 overflow-hidden">
    <div class="pre-footer-section">
        <a href="/contact">
            <h6 class="w-full max-w-8xl text-center text-red-500 mx-auto sm:pl-16 sm:pr-12 pre-footer-cta">Contact Us</h6>
            <hr class="w-full mb-3 pre-footer-title-line" />
            <hr class="w-full pre-footer-title-line" />
        </a>
    </div>
    <div class="relative flex items-start justify-between max-w-8xl mx-auto pt-8 sm:pt-12 pb-10 px-4 sm:px-6 z-20">
        <div class="flex flex-wrap w-7/12 sm:w-1/3 md:w-auto">
            <a href="/" class="w-full">
                <img class="w-12 md:w-8 pb-4" src="{{ logos[0].acf.footer_logo.url }}" />
            </a>
            <div class="md:flex pt-4">
                <div class="pr-6">
                    <a href="/vendor-submission/" class="privacy-text">
                        Vendor Submission
                    </a>
                </div>
                <div class="pt-4 md:pt-0">
                    <a href="/privacy-policy/" class="privacy-text">
                        Privacy Policy
                    </a>
                </div>
            </div>
        </div>
        <div class="w-full sm:w-2/3">
            <div class="md:flex flex-wrap text-right">
                <a href="https://goo.gl/maps/CVf7MrL1GKSYZMLv7" class="w-full sm:w-auto pb-8 ml-auto">
                    <address class="md:mr-16 lg:mr-24 leading-loose footer-address">
                        {% for item in address[0].acf.value_list %}
                            {% if item.type == "Headquarters" %}
                                {{ item.street }}<br />
                                {{ item.city }}, {{ item.state }} {{ item.zip }}
                            {% endif %}
                        {% endfor %}
                    </address>
                </a>
                <div class="w-full sm:w-auto md:mr-16 lg:mr-24 pb-8 footer-contact">
                    <a href="mailto:{% for item in email[0].acf.value_list %}{% if item.type == "Primary" %}{{ item.value }}{% endif %}{% endfor %}">
                        Email Us
