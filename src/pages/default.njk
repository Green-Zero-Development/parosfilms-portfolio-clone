---
layout: base.njk
pagination:
    data: wpages
    size: 1
    alias: wpage
eleventyComputed:
  yoastHead: "{{ wpage.yoast|safe }}"
permalink: /{{ wpage.slug }}/index.html
---
{%- for item in pagination.items %}
    {% if item.template == "templates/packages.php" %}
        {% include "pages/packages.njk" %}
    {% elseif item.template == "templates/about.php" %}
        {% include "pages/about.njk" %}
    {% elseif item.template == "templates/contact.php" %}
        {% include "pages/contact.njk" %}
    {% elseif item.template == "templates/portfolio.php" %}
        {% include "pages/portfolio.njk" %}
    {% elseif item.template == "templates/vendor-submission.php" %}
        {% include "pages/vendor-submission.njk" %}
    {% elseif item.template == "templates/raw-footage-request.php" %}
        {% include "pages/raw-footage-request.njk" %}
    {% elseif item.template == "templates/agreement.php" %}
        {% include "pages/agreement.njk" %}
    {% elseif item.template == "templates/privacy-policy.php" %}
        {% include "pages/privacy-policy.njk" %}
    {% elseif item.template == "templates/thank-you.php" %}
        {% include "pages/thank-you.njk" %}
    {% endif %}
{% endfor -%}